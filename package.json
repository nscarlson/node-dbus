{
    "name": "dbus",
    "version": "1.0.7",
    "description": "A D-Bus binding for Node",
    "author": "Shouqun Liu <liushouqun@gmail.com>",
    "contributors": [
        "Fred Chien <cfsghost@gmail.com>",
        "Bryan Burgers <bryan@burgers.io>",
        "Artem Derevnjuk <art_dere@hotmail.com>",
        "Thiago Cardoso <thiago.figueredo@cesar.org.br>"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/Shouqun/node-dbus.git"
    },
    "engines": {
        "node": ">= 0.12.0"
    },
    "main": "build/lib/dbus",
    "exports": {
        ".": {
            "require": "./build/lib/dbus.js",
            "import": "./build/lib/dbus.js",
            "types": {
                "default": "./index.d.ts"
            }
        }
    },
    "types": "index.d.ts",
    "scripts": {
        "node-gyp": "node_modules/.bin/node-gyp",
        "install": "npm run build:release",
        "test": "jest --verbose --detectOpenHandles",
        "build:release": "node-gyp configure build",
        "build:dev": "node-gyp configure build --debug",
        "build": "tsc --build --verbose --force"
    },
    "dependencies": {
        "minimist": "1.2.5"
    },
    "os": [
        "!win32"
    ],
    "devDependencies": {
        "@jest/globals": "29.7.0",
        "@types/jest": "29.5.12",
        "@types/react": "^18.2.79",
        "jest": "29.7.0",
        "nan": "2.19.0",
        "node-gyp": "10.1.0",
        "source-map-support": "0.5.21",
        "tap": "18.7.2",
        "ts-jest": "29.1.2",
        "ts-node": "10.9.2",
        "typescript": "5.4.5"
    }
}
